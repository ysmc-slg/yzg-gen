<% if(elementType == 'select'){ %>
    <% if(config.resultMap != null){ %>
       <select id="${config.mapperElementId}" resultMap="${config.resultMap}">
    <% }%>
    <% if(config.resultType != null){ %>
       <select id="${config.mapperElementId}" resultType="${config.resultType}">
    <% }%>
        <![CDATA[
            ${sql}
        ]]>
    </select>
<% } %>